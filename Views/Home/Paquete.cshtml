@{
    ViewData["Title"] = "Paquetes";
}


<div class="container">
    <div class="row">
        <div class="col-4"></div>
        <div class="col-4">
            <img class="TapaMundial" src="https://http2.mlstatic.com/D_NQ_NP_859267-MLA51266807628_082022-O.jpg"
                alt="Paquete mundial">
        </div>
       
        
        
    </div>
    <div class="row">
        @foreach (var item in ViewBag.Jugadores)
        {
        <img class="fotos" src="@item.ImagenJugador" alt="@item.Nombre"  data-bs-toggle="modal" data-bs-target="#exampleModal2" onclick="MostrarMasInfo(@item.IdJugador)">  
        }
    </div>
</div>